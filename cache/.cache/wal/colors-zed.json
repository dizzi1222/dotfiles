{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0c050590",
                "editor.background": "#0c050590",
                "editor.foreground": "#c2c0c0",
                "text": "#c2c0c0",
                "text.muted": "#c2c0c070",
                "text.ignored": "#c2c0c040",
                "text.placeholder": "#c2c0c050",
                "ignored": "#c2c0c030",
                "element.hover": "#c2c0c030",
                "ghost_element.hover": "#AB1A1130",
                "ghost_element.selected": "#AB1A1130",
                "ghost_element.active": "#AB1A1160",
                "border": "#0c0505",
                "editor.highlighted_line.background": "#AB1A1110",
                "editor.active_line.background": "#AB1A1110",
                "panel.background": "#0c050590",
                "title_bar.background": "#0c050590",
                "title_bar.inactive_background": "#0c050590",
                "status_bar.background": "#0c050590",
                "drop_target.background": "#0c050590",
                "elevated_surface.background": "#0c0505",
                "toolbar.background": "#0c050590",
                "tab_bar.background": "#0c050590",
                "tab.inactive_background": "#0c050590",
                "tab.active_background": "#AB1A1130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0c0505",
                "scrollbar.thumb.background": "#AB1A11",
                "editor.gutter.background": "#0c050590",
                "terminal.background": "#0c050510",
                "terminal.foreground": "#c2c0c0",
                "terminal.dim_foreground": "#c2c0c0",
                "terminal.bright_foreground": "#c2c0c0",
                "terminal.ansi.black": "#0c0505",
                "terminal.ansi.red": "#AB1A11",
                "terminal.ansi.green": "#A32722",
                "terminal.ansi.yellow": "#D71F16",
                "terminal.ansi.blue": "#D62116",
                "terminal.ansi.magenta": "#A95E5B",
                "terminal.ansi.cyan": "#9E807E",
                "terminal.ansi.white": "#c2c0c0",
                "terminal.ansi.bright_black": "#675555",
                "terminal.ansi.bright_red": "#AB1A11",
                "terminal.ansi.bright_green": "#A32722",
                "terminal.ansi.bright_yellow": "#D71F16",
                "terminal.ansi.bright_blue": "#D62116",
                "terminal.ansi.bright_magenta": "#A95E5B",
                "terminal.ansi.bright_cyan": "#9E807E",
                "terminal.ansi.bright_white": "#c2c0c0",
                "modified": "#AB1A11",
                "syntax": {
                    "attribute": {
                        "color": "#AB1A11"
                    },
                    "boolean": {
                      "color": "#A32722"
                    },
                    "comment": {
                      "color": "#675555"
                    },
                    "comment.doc": {
                      "color": "#675555"
                    },
                    "constant": {
                      "color": "#A32722"
                    },
                    "function": {
                      "color": "#9E807E"
                    },
                    "keyword": {
                      "color": "#D62116"
                    },
                    "number": {
                      "color": "#A95E5B"
                    },
                    "operator": {
                      "color": "#D62116"
                    },
                    "string": {
                      "color": "#AB1A11"
                    },
                    "variable": {
                      "color": "#A32722"
                    }
                },
                "players": [
                  {
                    "cursor": "#AB1A11",
                    "background": "#0c0505",
                    "selection": "#AB1A1130"
                  }
                ]
            }
        }
    ]
}
